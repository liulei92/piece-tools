{
  "name": "piece-tools",
  "private": true,
  "version": "0.0.0",
  "files": [
    "dist"
  ],
  "main": "./dist/piece-tools.umd.cjs",
  "module": "./dist/piece-tools.mjs",
  "types": "./dist/piece-tools.d.ts",
  "exports": {
    "types": "./dist/piece-tools.d.ts",
    "import": "./dist/piece-tools.mjs",
    "require": "./dist/piece-tools.umd.cjs"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "commit": "cz",
    "release": "release-it",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@release-it/conventional-changelog": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "commitizen": "^4.3.0",
    "cz-customizable": "^7.2.1",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "release-it": "^17.6.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.2.10",
    "vite-plugin-dts": "^4.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.cjs"
    }
  }
